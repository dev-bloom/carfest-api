{
  "name": "obscure-list-api",
  "version": "0.0.1",
  "description": "Documents",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "16 || 18 || 20"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "docker:build": "docker build -t obscure-list-api .",
    "docker:run": "docker run -p 3000:3000 -d obscure-list-api",
    "premigrate": "yarn run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run rebuild",
    "start": "nodemon -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "yarn run clean && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Alejandro Chaves <github@alejandrochaves.dev>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^6.0.0",
    "@loopback/core": "^5.0.0",
    "@loopback/repository": "^6.0.0",
    "@loopback/rest": "^13.0.0",
    "@loopback/rest-explorer": "^6.0.0",
    "@loopback/service-proxy": "^6.0.0",
    "axios": "^1.4.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "firebase": "^9.22.2",
    "loopback-connector-mongodb": "^5.2.3",
    "nodemailer": "^6.9.3",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^10.0.0",
    "@loopback/eslint-config": "^14.0.0",
    "@loopback/testlab": "^6.0.0",
    "@types/node": "^14.18.47",
    "@types/nodemailer": "^6.4.8",
    "eslint": "^8.40.0",
    "nodemon": "^2.0.22",
    "source-map-support": "^0.5.21",
    "typescript": "~4.9.5"
  }
}
